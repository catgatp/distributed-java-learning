<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="geektime.spring.springbucks.mapper.OrderCoffeeMapper">

    <insert id="insertOrderCoffee" parameterType="map">
        INSERT INTO t_order_coffee (coffee_order_id, items_id)
        VALUES (#{orderId}, #{coffeeId})
    </insert>

</mapper>
